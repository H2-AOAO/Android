<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/> <!-- 배경색 투명 -->
            <stroke android:width="2dp" android:color="#E5E5E5"/> <!-- 테두리 색상과 두께 -->
            <corners android:radius="15dp"/> <!-- 모서리 둥글기 -->
        </shape>
    </item>
</selector>

